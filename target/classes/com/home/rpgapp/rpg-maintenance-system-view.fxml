<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="228.0" prefWidth="447.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.home.rpgapp.controller.RPGMaintenanceSystemController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="RPG Maintenance System">
      <font>
         <Font name="Book Antiqua" size="25.0" />
      </font>
   </Text>
   <GridPane maxHeight="500.0" maxWidth="300.0" minHeight="0.0" minWidth="0.0" prefHeight="123.0" prefWidth="302.0" VBox.vgrow="NEVER">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
          <Button onAction="#onManageCharactersClick" prefHeight="40.0" prefWidth="150.0" text="Manage Characters" />
          <Button alignment="CENTER" onAction="#onManageItemsClick" prefHeight="40.0" prefWidth="150.0" text="Manage Items" GridPane.columnIndex="1" />
          <Button alignment="CENTER" onAction="#onTransactionHistoryClick" prefHeight="40.0" prefWidth="150.0" text="Transaction History" GridPane.rowIndex="1" />
          <Button alignment="CENTER" onAction="#onSimulationAreaClick" prefHeight="40.0" prefWidth="150.0" text="Simulation Area" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      </children>
   </GridPane>
</VBox>
